Import C.Stdio

Proc *Main(Argc: Int32, Argv: Uint8^^): Int32
	i: Int32 Mut := 0
	While i < 100
		FizzBuzz(i)
		++i
	End

	Return 0
End

Proc FizzBuzz(n: Int32): Null
	Printed: Bool Mut := False

	If n % 3 == 0
		printf("fizz")
		Printed := True
	End

	If n % 5 == 0
		printf("buzz")
		Printed := True
	End

	If !printed
		printf("%d", n)
	End

	printf("\n");
End
