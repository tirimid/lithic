Import Std.Io

Proc *Main(Argc: Int32, Argv: Uint8^^): Int32
	Var i: Int32 Mut := 0
	While i < 100
		FizzBuzz(i)
		++i
	End

	Return 0
End

Proc FizzBuzz(n: Int32): Null
	Var Printed: Bool Mut := False

	If n % 3 == 0
		Print("fizz")
		Printed := True
	End

	If n % 5 == 0
		Print("buzz")
		Printed := True
	End

	If !Printed
		Print("{i}", n)
	End

	Print("\n")
End
