Import C.Stdio

Proc *Main(Argc: Int32, Argv: Uint8^^): Int32
	Var i: Int32 Mut := 0
	While i < 100
		FizzBuzz(i)
		++i
	End

	Return 0
End

Proc FizzBuzz(n: Int32)
	Var Printed: Bool Mut := False

	If n % 3 == 0
		printf(Base "fizz")
		Printed := True
	End

	If n % 5 == 0
		printf(Base "buzz")
		Printed := True
	End

	If !printed
		printf(Base "%d", n)
	End

	printf(Base "\n")
End
